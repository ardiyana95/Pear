@model DSLNG.PEAR.Web.ViewModels.CorporatePortofolio.IndexCorporatePortofolioViewModel
@{
    ViewBag.Title = "Index";
}

<h2>Pms Summary Configuration</h2>

<table class="table table-striped table-bordered table-responsive">
    <thead>
        <tr>
            <th rowspan="2">No</th>
            <th rowspan="2">Year</th>
            <th rowspan="2">Title</th>
            <th rowspan="2">Pillar & KPI</th>
            <th rowspan="2">Value Target</th>
            <th rowspan="2">Value Achievement</th>
            <th rowspan="2">Scoring Indicator</th>
            <th rowspan="2">Is Active</th>
        </tr>
    </thead>
    <tbody>
        @{
            int i = 0;   
        }
        @foreach (var item in Model.CorporatePortofolios)
        {
            i++;
            <tr data-toggle="modal">
                <td>@i</td>
                <td>@item.Year</td>
                <td>@item.Title</td>
                <td>@Html.ActionLink("Details", "PmsSummaryConfiguration", "CorporatePortofolio", new { id = item.Id }, null)</td>
                <td>@Html.ActionLink("Details", "Create", "KpiTarget")</td>
                <td>@*@Html.ActionLink("Details", "Details", "CorporatePortofolio", new { id = item.Id })*@ Details</td>
                <td>@*@Html.ActionLink("Details", "Details", "CorporatePortofolio", new { id = item.Id })*@ Details</td>
                <td>@item.IsActive</td>
            </tr>
        }
    </tbody>
</table>

