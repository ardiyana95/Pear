@model DSLNG.PEAR.Web.ViewModels.Der.Display.DisplayKpiInformationViewModel

<table class="table table-bordered indicative">
    <thead>
    <tr>
        <td colspan="3" style="font-size:11px;padding:18px; ">
            DSLNG Indicative Commercial Price (JCC based)-USD/mmbtu
        </td>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td style="width: 34%">
            Feed Gas (GSA)
        </td>
        <td>
            JOB @Html.DisplayCompleteDerValue(Model.KpiInformationViewModels.Single(x => x.Position == 0).DerItemValue.Value, string.Empty, string.Empty)
