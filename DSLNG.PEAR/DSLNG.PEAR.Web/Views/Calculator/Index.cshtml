    <!-- required for floating -->
    <!-- Nav tabs -->
    <ul class="nav nav-tabs tabs-left">
        <li class="active"><a href="#productionYield" data-toggle="tab">Production Yield</a></li>
        <li><a href="#pricing" data-toggle="tab" class="vertical-tab">Pricing</a></li>
