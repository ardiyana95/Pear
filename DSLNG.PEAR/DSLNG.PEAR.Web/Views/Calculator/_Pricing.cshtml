@{
    ViewBag.Title = "_Pricing";
}
<div class="row">
    <div class="col-md-6">
        <h3>Pricing</h3>
    </div>
    <div class="col-md-6">
        <h3>Constanta</h3>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <form class="form-horizontal">
            <div class="form-group">
                <label class="col-sm-2 control-label">JCC Price</label>
                <div class="col-sm-6">
                    <input type="text" class="form-control" id="jcc-price" placeholder="JCC Price">
                </div>
                <div class="col-sm-4">
                    <select class="form-control">
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                        <option>5</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 control-label">Result</label>
                <div class="col-sm-6">
                    <input type="text" class="form-control" id="jcc-price" placeholder="JCC Price">
                </div>
                <div class="col-sm-4">
                    <label class="control-label">LNG SPA FOB</label>    
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-6 col-md-offset-2">
                    <input type="text" class="form-control" id="jcc-price" placeholder="JCC Price">
                </div>
                <div class="col-sm-4">
                    <label class="control-label">LNG SPA DES</label>    
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-6 col-md-offset-2">
                    <input type="text" class="form-control" id="jcc-price" placeholder="JCC Price">
                </div>
                <div class="col-sm-4">
                    <label class="control-label">Senoro Feed Gas</label>    
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-6 col-md-offset-2">
                    <input type="text" class="form-control" id="jcc-price" placeholder="JCC Price">
                </div>
                <div class="col-sm-4">
                    <label class="control-label">Matindok Feed Gas</label>    
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-6 col-md-offset-2">
                    <input type="text" class="form-control" id="jcc-price" placeholder="JCC Price">
                </div>
                <div class="col-sm-4">
                    <label class="control-label">Average Feed Gas</label>    
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-6 col-md-offset-2">
                    <input type="text" class="form-control" id="jcc-price" placeholder="JCC Price">
                </div>
                <div class="col-sm-4">
                    <label class="control-label">CDS Price</label>    
                </div>
            </div>
        </form>
    </div>
    <div class="col-md-6">
        @*<h3>Constanta</h3>*@
    </div>
</div>

